{
    "manifest_version": 2,
    "name": "CCBot",
    "version": "0.28",
    "description": "Расширение для озвучки особых сообщений чата(или всех сообщений), а также обработки особых команд в чате.",
    "icons": {"48":"icon.png"},
    "permissions": [
        "storage",
        "<all_urls>",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.crazycash.tv/*"],
            "js": ["js/content.js"],
            "run_at": "document_end"
        }
    ]
}